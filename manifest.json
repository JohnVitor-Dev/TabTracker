{
    "name": "Tab Tracker",
    "description": "A browser extension for quickly saving and managing your favorite URLs.",
    "manifest_version": 3,
    "version": "1.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "192": "/android-chrome-192x192.png",
        "512": "/android-chrome-512x512.png"
    }
}